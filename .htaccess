<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /visualtalks/
RewriteRule ^index\.php$ - [L]
RewriteRule ^assets/(.*) /visualtalks/wp-content/themes/visualtalks/assets/$1 [QSA,L]
RewriteRule ^plugins/(.*) /visualtalks/wp-content/plugins/$1 [QSA,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /visualtalks/index.php [L]
</IfModule>

